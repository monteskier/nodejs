<!DOCTYPE html>
<html lang="es">

<head>
    <% include ../partials/head %>
</head>
</head>

<body>
    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="jumbotron">

            <h1>Medalla de Sant Vicenç 2017 </h1>
            <p>Estàs el AdminSite</p>
        </div>
        <p>Llista d'Aspirants:</p>
        <div id="flex-cont">
          <ul>
          <% aspirants.forEach(function(aspirant) { %>
            <li><%= aspirant.nom %> <button type="button" class="btn btn-danger" value="<%= aspirant._id %>">Eliminar</button></li>
            <% }); %>
          </ul>
        </div>

    </main>
    <footer>
        <% include ../partials/footer %>
    </footer>

</body>
<%include ../partials/scripts %>

</html>
